{
  "name": "eleventy-web-starter",
  "version": "2.0.0",
  "description": "Robust Eleventy Boilerplate",
  "scripts": {
		"clean": "del build",
    "dev:eleventy": "eleventy --serve",
    "dev:postcss": "postcss source/_assets/css/styles.css -o source/_includes/_css/styles.css -w --verbose",
    "build:postcss": "NODE_ENV=production postcss source/_assets/css/styles.css -o source/_includes/_css/styles.css --verbose",
    "build:eleventy": "eleventy",
    "start": "run-p dev:*",
    "build": "run-s clean build:*"
  },
  "keywords": [
    "eleventy",
    "11ty",
    "html5",
    "boilerplate"
  ],
  "author": "Chris Collins",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-cache-assets": "^2.2.1",
    "@11ty/eleventy-img": "^0.9.0",
    "autoprefixer": "^10.2.6",
    "clean-css": "^5.1.2",
    "cssnano": "^5.0.5",
    "del-cli": "^3.0.1",
    "html-minifier": "^4.0.0",
    "luxon": "^1.26.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.0",
    "postcss-cli": "^8.3.1",
    "terser": "^5.7.0"
  }
}
